package test_3;

import java.util.HashMap;
import java.util.Map;

public class App {
	public static void main(String[] args) {
		Map<Integer, String> hashMap = new HashMap<Integer, String>();
		
		hashMap.put(1, "abc");
		hashMap.put(2, "efg");
		hashMap.put(3, "hij");
		hashMap.put(4, "opq");
		
		System.out.println(hashMap);
		
		System.out.println("*****************************************");

		System.out.println(hashMap.size());
		
		System.out.println("*****************************************");

		System.out.println(hashMap.keySet());
		
		System.out.println("*****************************************");

		System.out.println(hashMap.values());
		
		System.out.println("*****************************************");

		for (Integer temp: hashMap.keySet()) {
			System.out.println(hashMap.get(temp));
		}
		
		System.out.println("*****************************************");

		System.out.println(hashMap.containsKey(3));
		
		System.out.println("*****************************************");

		System.out.println(hashMap.containsValue("efg"));
		
		System.out.println("*****************************************");

		System.out.println(hashMap.remove(3));
		
		System.out.println(hashMap);
		
		System.out.println("*****************************************");
		
		System.out.println(hashMap.remove(1, "abc"));
		
		System.out.println(hashMap);
		
		System.out.println("*****************************************");
		
		hashMap.remove(2, "abc")
		

	}
}
